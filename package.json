        "js-cookie": "^2.2.1",
        "linq": "^3.2.2",
        "moment": "^2.27.0",
        "popper.js": "^1.16.1",
        "vform": "^1.0.1",
        "vue-router": "^3.4.3",
        "vue-scrollto": "^2.18.2",
        "vuex": "^3.5.1",
        "vuex-router-sync": "^5.0.0"
